from random import random

matriz = np.array([[random(), 4,6], [5.5,9.8, random()]]) 
print(matriz) 
print(matriz.shape) 


from random import random

matriz = np.array([[random(),4,np.sqrt(2)], [pi,9.8,random()]]) 
print(matriz) 
print(matriz.shape) 



# sympy: Calculos simbolico (sympy gamma.com) 




A = matriz
print(A) 


# Transponer A 
B = A.T
print(B) 


# Multiplicar A por B simbolo para multiplicar matrices es con el  A@B
C = A@B
print(C) 


