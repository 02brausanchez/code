numero = int(input("¿Que tabla de multiplicar quieres saber?"))
print("Tu tabla del 1 al 10 es: ")  
for i in range(1,11): 
    print(numero, "x", i,"=",numero*i)


num = int(input("Ingrese el numero que quieres que tenga tu tabla:")) 
limite = int(input("Hasta que numero quieres que multiplique tu tabla:")) 
for i in range(limite): 
    print(f'{num} x {i} = {limite*i:2}')
[0]*10 

num = int(input("Ingrese el numero que quieres que tenga tu tabla:")) 
limite_inicial = int(input("Dame el limite inicial para tu tabla:"))
limite_final = int(input("Dame el limite final para tu tabla:"))

for i in range(limite_inicial,limite_final+1): 
    print(f'{num} x {i} = {num*i:2}')

